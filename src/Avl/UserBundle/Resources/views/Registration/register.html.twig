{% extends "UserBundle::layout.html.twig" %}

{% block content %}
<div class="col-md-12">
    {% if currentRouteName == 'avl_subuser_new' %}
        <h1>{{ 'title.subuser.registration'|trans }}</h1>
    {% else %}
        <h1>{{ 'title.registration'|trans }}  <small>{{ 'title.sub.registration'|trans }}</small></h1>
    {% endif %}
    {% include "FOSUserBundle:Registration:register_content.html.twig" %}
</div>
{% endblock %}